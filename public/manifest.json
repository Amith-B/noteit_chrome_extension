{
  "name": "NoteIt",
  "description": "Extension to take notes",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    }
  }
}
